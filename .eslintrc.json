{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".glsl"]
      }
    }
  },
  "rules": {
    "import/no-unresolved": [
      "error",
      {
        "ignore": [".glsl"]
      }
    ]
  },
  "ignorePatterns": ["**/*.glsl"]
}
